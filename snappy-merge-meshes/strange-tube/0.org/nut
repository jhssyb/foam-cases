FoamFile
{
  version 2.0;
  format ascii;
  class volScalarField;
  location "0";
  object nut;
}

dimensions [0 2 -1 0 0 0 0];
internalField uniform 0;

boundaryField
{
  inlet
  {
    type calculated;
    value uniform 0;
  }

  outlet
  {
    type calculated;
    value uniform 0;
  }

  tube-walls
  {
    type nutkWallFunction;
    value $internalField;
  }
}

